{
  "name": "wallet-address-book",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "up": "docker-compose up -d",
    "fresh": "yarn install && docker-compose down -v --remove-orphans && docker-compose up --build -d && sleep 20 && yarn schema && yarn generate",
    "new": "yarn install && docker-compose down --remove-orphans && docker-compose up --build -d && sleep 20 && yarn schema && yarn generate",
    "stop": "docker-compose stop",
    "schema": "curl -X POST localhost:8080/admin/schema --data-binary '@dgraph/schema.graphql'",
    "generate": ". ./.env && graphql-codegen && typechain --target=ethers-v5 types/web3",
    "dev": "next dev",
    "build": "next build",
    "generate-production": ". ./prod.env && graphql-codegen && typechain --target=ethers-v5 types/web3",
    "build-production": ". ./prod.env && graphql-codegen && typechain --target=ethers-v5 types/web3 && next build",
    "start": "next start",
    "lint": "yarn run js-lint && yarn run style-lint && yarn run type-check",
    "fix": "yarn run fix-js-lint && yarn run fix-style-lint",
    "fix-js-lint": "yarn run js-lint-src -- --fix && yarn run js-lint-tests -- --fix",
    "js-lint-src": "eslint -c config/.eslintrc.yml '*.ts' '**/*.tsx' -f table",
    "js-lint-tests": "eslint -c config/.eslintrc-test.yml 'test/js/**/*.js' 'test/js/**/*.ts' -f table",
    "js-lint": "yarn run js-lint-src && yarn run js-lint-tests && yarn run type-coverage",
    "test:unit": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.2",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-react-query": "^3.3.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/react-dom": "^17.0.11",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "@web3-react/walletlink-connector": "^6.2.8",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "ethers": "^5.5.3",
    "formik": "^2.2.9",
    "fortawesome": "^0.0.1-security",
    "graphql": "^16.2.0",
    "next": "^12.0.7",
    "postcss": "^8.4.5",
    "postcss-preset-env": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-use": "^17.3.2",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.0.12",
    "tslint": "^6.1.3",
    "typechain": "^6.1.0",
    "types": "^0.1.1",
    "typescript": "^4.5.4",
    "typescript-eslint": "^0.0.1-alpha.0",
    "web3-react": "^5.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@netlify/plugin-nextjs": "^4.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  }
}
